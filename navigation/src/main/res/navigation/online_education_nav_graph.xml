<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/online_education_nav_graph"
    app:startDestination="@id/home_nav_graph"
    >

    <include app:graph="@navigation/home_nav_graph" />
    <include app:graph="@navigation/profile_nav_graph" />
    <include app:graph="@navigation/entrance_nav_graph" />
    <include app:graph="@navigation/search_nav_graph" />
    <include app:graph="@navigation/saved_nav_graph" />

    <action
        android:id="@+id/toHome"
        app:destination="@id/home_nav_graph">
    </action>

    <action
        android:id="@+id/toProfile"
        app:destination="@id/profile_nav_graph">
    </action>

    <action
        android:id="@+id/toSearch"
        app:destination="@id/search_nav_graph">
    </action>

    <action
        android:id="@+id/toSaved"
        app:destination="@id/saved_nav_graph">
    </action>

    <action
        android:id="@+id/toEntrance"
        app:destination="@id/entrance_nav_graph">
    </action>

</navigation>